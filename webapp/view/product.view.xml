
<mvc:View xmlns:table="sap.ui.table" controllerName="northwind.controller.product"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <Table id="_IDGenTable" items="{
			path: '/Products'
			}">
            <columns>
                <Column id="_IDGenColumn9" >
                    <Text id="_IDGenText20"  text="{i18n>ProductID}" />
                </Column>
                <Column id="_IDGenColumn">
                    <Text id="_IDGenText" text="{i18n>ProductName}" />
                </Column>

                <Column id="_IDGenColumn1">
                    <Text id="_IDGenText1" text="{i18n>SupplierID}" />
                </Column>
                <Column id="_IDGenColumn2">
                    <Text id="_IDGenText2" text="{i18n>CategoryID}" />
                </Column>
                <Column id="_IDGenColumn3">
                    <Text id="_IDGenText3" text="{i18n>QuantityPerUnit}" />
                </Column>
                <Column id="_IDGenColumn4">
                    <Text id="_IDGenText4" text="{i18n>UnitPrice}" />
                </Column>
                <Column id="_IDGenColumn5">
                    <Text id="_IDGenText5" text="{i18n>UnitsInStock}" />
                </Column>
                <Column id="_IDGenColumn6">
                    <Text id="_IDGenText6" text="{i18n>UnitsOnOrder}" />
                </Column>
                <Column id="_IDGenColumn7">
                    <Text id="_IDGenText7" text="{i18n>ReorderLevel}" />
                </Column>
                <Column id="_IDGenColumn8">
                    <Text id="_IDGenText8" text="{i18n>Discontinued}" />
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1"  type="Navigation" press=".onItemPress">
                    <cells>
                        <Text id="_IDGenText18"  text="{ProductID}" />
                        <Text id="_IDGenText9"  text="{ProductName}" />
                        <Text id="_IDGenText10"  text="{SupplierID}" />
                        <Text id="_IDGenText11"  text="{CategoryID}" />
                        <Text id="_IDGenText12"  text="{QuantityPerUnit}" />
                        <Text id="_IDGenText13"  text="{UnitPrice}" />
                        <Text id="_IDGenText14"  text="{UnitsInStock}" />
                        <Text id="_IDGenText15"  text="{UnitsOnOrder}" />
                        <Text id="_IDGenText16"  text="{ReorderLevel}" />
                        <Text id="_IDGenText17"  text="{Discontinued}" />
                    </cells>
                </ColumnListItem>
            </items>

        </Table>
    </Page>
</mvc:View>
